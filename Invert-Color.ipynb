   "source": [
    "from PIL import Image\n",
    "import PIL.ImageOps\n",
    "# 读入图片\n",
    "image = Image.open('typography.png')\n",
    "# 反转\n",
    "inverted_image = PIL.ImageOps.invert(image)\n",
    "# 保存图片\n",
    "inverted_image.save('typography-inverted.png')"
   ]
  }
